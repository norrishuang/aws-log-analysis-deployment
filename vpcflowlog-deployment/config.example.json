{
  "vpcId": "vpc-12345678",
  "bucketName": "my-vpc-flow-logs-bucket",
  "environment": "prod",
  "aws": {
    "account": "123456789012",
    "region": "us-east-1"
  },
  "flowLogs": {
    "trafficType": "ALL",
    "logFormat": "custom",
    "deliveryRoleArn": "auto-create"
  },
  "s3": {
    "lifecycleRules": {
      "transitionToIA": 30,
      "transitionToGlacier": 90,
      "transitionToDeepArchive": 365,
      "deleteOldVersions": 30
    },
    "encryption": "S3_MANAGED",
    "versioning": true
  },
  "sqs": {
    "enableNotification": true,
    "queueName": "vpc-flow-logs-notifications",
    "visibilityTimeoutSeconds": 300,
    "messageRetentionPeriod": 1209600,
    "deadLetterQueue": {
      "enabled": true,
      "maxReceiveCount": 3,
      "retentionPeriod": 1209600
    },
    "eventFilters": {
      "prefix": "vpc-flow-logs/",
      "suffix": ".gz"
    }
  },
  "cloudWatch": {
    "logGroupName": "/aws/vpc/flowlogs",
    "retentionDays": 30,
    "trafficType": "REJECT"
  },
  "processing": {
    "glue": {
      "database": "vpc_flow_logs",
      "table": "flow_logs",
      "autoCreatePartitions": true
    },
    "athena": {
      "workgroup": "primary",
      "outputLocation": "s3://my-athena-results-bucket/vpc-flow-logs/"
    }
  }
}