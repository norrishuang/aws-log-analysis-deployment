{
    "index_patterns": [
      "vpc-logs-*"
    ],
    "template": {
      "settings": {
        "index.codec": "zstd",
        "index.refresh_interval": "10s",
        "index.number_of_shards": "3",
        "index.translog.flush_threshold_size": "3072mb",
        "index.translog.sync_interval": "30s",
        "index.translog.durability": "async",
        "index.number_of_replicas": "0",
        "index.plugins.index_state_management.rollover_alias": "vpc-logs"
      },
      "mappings": {
        "properties": {
          "srcaddr": {
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "keyword"
              }
            },
            "type": "text"
          },
          "dstport": {
            "type": "keyword"
          },
          "traffic-path": {
            "type": "keyword"
          },
          "subnet-id": {
            "type": "keyword"
          },
          "dstaddr": {
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "keyword"
              }
            },
            "type": "text"
          },
          "pkt-srcaddr": {
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "keyword"
              }
            },
            "type": "text"
          },
          "type": {
            "type": "keyword"
          },
          "vpc-id": {
            "type": "keyword"
          },
          "sublocation-type": {
            "type": "keyword"
          },
          "packets": {
            "type": "long"
          },
          "protocol": {
            "type": "keyword"
          },
          "pkt-dstaddr": {
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "keyword"
              }
            },
            "type": "text"
          },
          "end": {
            "type": "date"
          },
          "pkt-dst-aws-service": {
            "type": "keyword"
          },
          "log-status": {
            "type": "keyword"
          },
          "sublocation-id": {
            "type": "keyword"
          },
          "az-id": {
            "type": "keyword"
          },
