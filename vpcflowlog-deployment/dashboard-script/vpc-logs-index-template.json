          "region": {
            "type": "keyword"
          },
          "interface-id": {
            "type": "keyword"
          }
        }
      },
      "aliases": {
        "vpc-logs": {}
      }
    },
    "composed_of": [],
    "priority": "10",
    "_meta": {
      "flow": "simple"
    },
    "name": "vpc_logs_template"
